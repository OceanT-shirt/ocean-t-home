{
  "private": true,
  "scripts": {
    "dev": "concurrently 'next dev --turbo' 'npm run tailwind --watch'",
    "dev:notailwind": "next dev --turbo",
    "dev:noturbo": "concurrently 'next dev' 'npm run tailwind --watch'",
    "build": "next build",
    "start": "next start -p $PORT",
